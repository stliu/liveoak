<module xmlns="urn:jboss:module:1.1" name="org.hibernate">
    <resources>
        <artifact name="org.hibernate:hibernate-core:${hibernate.version}"/>
        <artifact name="org.hibernate:hibernate-entitymanager:${hibernate.version}"/>
        <artifact name="org.hibernate.common:hibernate-commons-annotations:${hibernate.annotations.version}"/>
    </resources>

    <dependencies>
        <module name="javax.api"/>
        <module name="javax.persistence.api" export="true"/>
        <module name="javax.transaction.api"/>
        <module name="org.dom4j"/>
        <module name="org.jboss.logging"/>
        <module name="org.javassist" export="true"/>
        <module name="org.antlr"/>
    </dependencies>
</module>
